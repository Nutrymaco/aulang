const a = 55;
const b = 12;

fun min(first, second) = {
    print(first);
    print(second);
    if (first < second) {
        const c = first;
    } else {
        const c = second;
    }

    return c;
}

fun minWithFive(second) = {
    const five = 5;
    const res = min(five, second);
    return res;
}

const lessThan5 = minWithFive(a);
print(lessThan5);